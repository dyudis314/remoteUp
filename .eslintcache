,\n        // e.g. this way you end up with [\"developer\", \"golang\", \"js\"] instead of [\"developer\", [\"golang\", \"js\"]]\n       // validFields = validFields.concat(j.tags);\n        console.log(\"searching fields\", validFields, \"for job\", j);\n        validFields.forEach((f) => {\n            f = f.toLowerCase();\n            if (f.includes(queryLowerCase) || queryLowerCase.includes(f)) { \n              foundJobs.push(j);\n            }\n        });\n    });\n\n    let totalResults = foundJobs.length;\n    let message = `Found ${totalResults} jobs that match query ${query}`\n    console.debug(message);\n    console.log(\"Setting state, this is\", this);\n    this.setState( {\n      results: foundJobs,\n      message: message,\n      totalResults: totalResults,\n      loading: false,\n  } )\n}\n\n  handleOnInputChange = (event) => {\n  const query = event.target.value;\n  if (! query ) {\n    this.setState( {query, results: {}, message: '' });\n   } else { \n      this.findResults(query);\n    }\n  };\n  \n  renderSearchResults = () => {\n    const { results } = this.state;\n    console.log(\"Rendering search results with results\", typeof(results));\n    if (results.length == 0) {\n      return;\n    }\n    \n      return (   \n        \n        <div className=\"results-container\">\n            <Container>\n              {_.uniqBy(results).map((result, index) => {\n                return <div>\n                        {ResultCard(result)}\n                       </div>\n              })}             \n            </Container>\n      </div>  \n      )\n  };\n\n                /* Pagination Component */\n  /*\n          <Pagination bsName=\"test\" size=\"sm\">\n\n        {_.uniqBy(results).map((result, index) => {\n            <Pagination.Item \n              key={index} \n              active={(index + 1 === this.state.activePage)}>\n                    {ResultCard(result)}\n            </Pagination.Item>\n          </Pagination>\n  */\n  \n\n  render() {\n\n    const { loading, results } = this.state;\n  \n    return (\n      <div>\n        {/* Heading */}\n        <div className=\"heading\">\n            <div className=\"heading-text\">\n              \n          <h1>remoteUp</h1>\n          <h5>Work In Tech From Anywhere</h5>\n            </div>\n        {/* Search Input */}\n        <Form>\n            <Col>  \n              <Form.Control \n              placeholder={`Search ${this.props.jobs.length} remote tech jobs...`}\n              className=\"searchbox\"\n              type=\"text\"\n              name=\"query\"\n              value={this.query}\n              id=\"search-input\"\n              onChange = {_.debounce(this.handleOnInputChange, 150)}/>\n              <i className=\"fas fa-search search-icon\"/>\n            </Col>  \n        </Form>\n        </div>\n\n      {/* Results Row */}\n        <Row>\n            {this.renderSearchResults()}\n        </Row> \n\n      </div>\n    )\n  }\n}\n\nexport default Search;","/Users/Dylan/Desktop/remoteup2/remoteup1/src/Jobs.js",[],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/components/Card.js",["82"],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/components/Footer.js",["83","84","85","86","87"],"import React from 'react';\nimport { MDBCol, MDBContainer, MDBRow, MDBFooter } from \"mdbreact\";\n\nconst Footer = () => {\n  return ( \n<MDBFooter color=\"blue\" className=\"footer font-small pt-4 mt-4\">\n<MDBContainer fluid className=\"text-center text-md-left\">\n  <MDBRow>\n    <MDBCol md=\"6\">\n      <p>\n        This application is powered by the <a target=\"_blank\" href=\"https://www.producthunt.com/posts/remote-jobs-api\">Remote OK Jobs API.</a>\n        <br></br>\n      </p>\n    </MDBCol>\n    <MDBCol md=\"6\">\n      <ul>\n        <li className=\"list-unstyled\">\n          \n        </li>\n        <li className=\"list-unstyled\">\n          <a target=\"_blank\" href=\"https://twitter.com/home\">Twitter</a>\n        </li>\n        <li className=\"lis