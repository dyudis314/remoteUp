 1; i <= Math.ceil(totalPosts / postsPerPage); i++) {\n    pageNumbers.push(i);\n  }\n\n    return (\n        <nav>\n          <ul className=\"pagination justify-content-center\">\n            <li className=\"page-item\">\n          <a className=\"page-link\" href=\"#\" onClick = {() => prevPage()}>Previous</a>\n        </li>\n        {pageNumbers.map(num => (\n            <li className=\"page-item\" key={num}>\n              <a onClick={() => paginate(num)} href=\"#\" className=\"page-link\">{num}</a>\n            </li>\n          ))}\n        <li className=\"page-item\">\n          <a className=\"page-link\" href=\"#\" onClick = {() => prevPage()}>Next</a>\n        </li>\n          </ul>\n        </nav>\n      )\n    }\n  }\n\nexport default Pagination;",["71","72"],"/Users/Dylan/remoteup3/src/components/Results.js",["73","74","75","76"],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/index.js",[],["77","78"],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/App.js",[],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/reportWebVitals.js",[],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/components/Search.js",["79","80","81","82"],"import React from 'react'; \nimport './Search.css';\nimport './Results.css'\nimport { Container, Form, Col, Row } from 'react-bootstrap';\nimport ResultCard from './Card';\nimport AutoComplete from './AutoComplete.Js'\nconst _ = require(\"lodash\"); \n\nclass Search extends React.Component {\n\n\n  constructor( props ) {\n      super( props );\n      \n      this.state = {\n        query: '',\n        results: [],\n        loading: false,\n        message: '',\n        activePage: 1\n      };\n      \n      this.remoteJobs = this.props.jobs;\n  }\n\n  findResults(query) {\n    this.setState({\n      loading: true\n    })\n    let queryLowerCase = query.toLowerCase();\n    let foundJobs = [];\n    this.props.jobs.forEach((j) => {\n        let validFields = [\n          j.position\n        ]\n        // Add all of the tags individually to validFields,\n        // e.g. this way you end up with [\"developer\", \"golang\", \"js\"] instead of [\"developer\", [\"golang\", \"js\"]]\n       // validFields = validFields.concat(j.tags);\n        console.log(\"searching fields\", validFields, \"for job\", j);\n        validFields.forEach((f) => {\n            f = f.toLowerCase();\n            if (f.includes(queryLowerCase) || queryLowerCase.includes(f)) { \n              foundJobs.push(j);\n            }\n        });\n    });\n\n    let totalResults = foundJobs.length;\n    let message = `Found ${totalResults} jobs that match query ${query}`\n    console.debug(message);\n    console.log(\"Setting state, this is\", this);\n    this.setState( {\n      results: foundJobs,\n      message: message,\n      totalResults: totalResults,\n      loading: false,\n  } )\n}\n\n  handleOnInputChange = (event) => {\n  const query = event.target.value;\n  if (! query ) {\n    this.setState( {query, results: {}, message: '' });\n   } else { \n      this.findResults(query);\n    }\n  };\n  \n  renderSearchResults = () => {\n    const { results } = this.state;\n    console.log(\"Rendering search results with results\", typeof(results));\n    if (results.length == 0) {\n      return;\n    }\n    \n      return (   \n        \n        <div className=\"results-container\">\n            <Container>\n              {_.uniqBy(results).map((result, index) => {\n                return <div>\n                        {ResultCard(result)}\n                       </div>\n              })}             \n            </Container>\n      </div>  \n      )\n  };\n\n                /* Pagination Component */\n  /*\n          <Pagination bsName=\"test\" size=\"sm\">\n\n        {_.uniqBy(results).map((result, index) => {\n            <Pagination.Item \n              key={index} \n              active={(index + 1 === this.state.activePage)}>\n                    {ResultCard(result)}\n            </Pagination.Item>\n          </Pagination>\n  */\n  \n\n  render() {\n\n    const { loading, results } = this.state;\n  \n    return (\n      <div>\n        {/* Heading */}\n        <div className=\"heading\">\n            <div className=\"heading-text\">\n              \n          <h1>remoteUp</h1>\n          <h5>Work In Tech From Anywhere</h5>\n            </div>\n        {/* Search Input */}\n        <Form>\n            <Col>  \n              <Form.Control \n              placeholder={`Search ${this.props.jobs.length} remote tech jobs...`}\n              className=\"searchbox\"\n              type=\"text\"\n              name=\"query\"\n              value={this.query}\n              id=\"search-input\"\n              onChange = {_.debounce(this.handleOnInputChange, 150)}/>\n              <i className=\"fas fa-search search-icon\"/>\n            </Col>  \n        </Form>\n        </div>\n\n      {/* Results Row */}\n        <Row>\n            {this.renderSearchResults()}\n        </Row> \n\n      </div>\n    )\n  }\n}\n\nexport default Search;","/Users/Dylan/Desktop/remoteup2/remoteup1/src/Jobs.js",[],"/Users/Dylan/Desktop/remoteup2/remoteup1/src/components/Card.js",["83"],"import React from 'react';\nimport { Card, Accordion, Button, Badge } from 'react-bootstrap';\nimport ScrollAnimation from 'react-animate-on-scroll';\n\nconst ResultCard = (result) => {\n  if (! result.company_name == \"\") {\n  return ( \n<ScrollAnimation\ndelay={150}\nanimateIn=\"fadeIn\"\nanimateOut='fadeOut'>\n    <Card \n    href={result.link} \n    target=\"_blank\" className=\"result-item\"\n    variant=\"success\"\n   >\n  <Card.Img variant=\"top\" src={result.company_logo} />\n\n    <Card.Body>\n      <Card.Title className=\"job-title\"><i>\n      {result.company_name}</i> | {result.position}\n      <h5>\n        <Badge \n        className=\"location\"\n        variant=\"secondary\">{result.location}                \n      </Badge>\n      </h5>\n      </Card.Title>\n      <Card.Text>\n        <p className=\"tags\">\n          <i>\n        #{result.t