sage = `Found ${totalResults} jobs that match query ${query}`\n    console.debug(message);\n    this.setState( {\n      results: foundJobs,\n      message: message,\n      totalResults: totalResults,\n      loading: false,\n  } )\n}\n\n  handleOnInputChange = (event) => {\n  const query = event.target.value;\n  if (! query ) {\n    this.setState( {query, results: {}, message: '' });\n   } else { \n      this.findResults(query);\n    }\n  };\n  \n\n  /* Pagination */\n  handlePageChange = (pageNumber) => {\n    console.log(`active page is ${pageNumber}`);\n    this.setState( {\n      activePage: pageNumber      \n    } );\n  }\n  \n  renderSearchResults = () => {\n    const {results } = this.state;\n    const defaultImg = \"../img/default.jpg\"\n\n    if (Object.keys(results).length && results.length) {\n     \n        for (let number = 1; number <= 5;  number ++) {\n          \n        console.log(\"First result\", results[0]);\n      return (\n        \n        \n        <div className=\"results-container\">\n\n            <Pagination\n            className=\"pagination\"\n            activePage={this.state.activePage}\n            itemsCountPerPage={10}\n            totalItemsCount={results.length}\n            pageRangeDisplayed={ Math.ceil(results.length/10) }\n            onChange={this.handlePageChange}\n            />\n            \n\n          { /* Lodash Library method to filter out duplicate results */}\n         \n            { _.uniqBy(results).map((result, index) => {\n            return (\n              \n              \n              <Card \n              key={index} \n              href={result.link} \n              target=\"_blank\" className=\"result-item\"\n              variant=\"success\"\n             >\n            <Card.Img \n             variant=\"top\"\n             src = {result.company_logo}\n             onerror={this.src= defaultImg}             \n             />\n\n              <Card.Body>\n                <Card.Title className=\"job-title\"><i>\n                {\n                result.company_name !==\n                 \"\"\n                 ?\n                 result.company_name\n                 :\n                 \"Company Name Not Found\"\n                }</i> | {result.position}\n                </Card.Title>\n                <Card.Text>\n                  <p className=\"tags\">\n                    <i>\n                  #{result.tags[0]} #{result.tags[1]} #{result.tags[2]}\n                  </i>\n                  </p>\n                   <p className=\"posted-on\">Posted on {result.posted_on}\n                   </p>\n                </Card.Text>\n\n                <Accordion>\n                  <Card>\n                    <Card.Header>\n                      <Accordion.Toggle as={Button} variant=\"link\" eventKey=\"0\">\n                        Learn More\n                          <Button\n                          target=\"_blank\" \n                          variant=\"primary\" \n                          size=\"sm\"\n                          className=\"apply-here\"\n                          href={result.link}>\n                            Apply\n                        </Button>\n                      </Accordion.Toggle>\n                    </Card.Header>\n                    <Accordion.Collapse eventKey=\"0\">\n                      <Card.Body>{result.description}\n                      <br></br><br></br>\n                        <Button \n                        variant=\"primary\" \n                        size=\"sm\"\n                        href={result.link}\n                        target=\"_blank\">\n                        Full Description\n                        </Button>\n                      </Card.Body> \n                    </Accordion.Collapse>\n                    <Card.Footer>\n                      <small>Source: <i>{result.source}</i>\n                      </small>\n                    </Card.Footer>\n                  </Card>\n                </Accordion>\n                </Card.Body>\n            </Card>     \n          )\n        }) }\n      </div>\n      \n      )\n    } \n  }\n};\n\n\n  render() {\n    const { loading } = this.state;\n    \n    console.log(`Rendering, local loading is ${loading}, state loading is ${this.state.loading}`);\n    \n    return (\n      <div>\n      <Row>\n        {/* Heading */}\n        <div className=\"heading\">\n            <div className=\"heading-text\">\n          <h1>remoteUp</h1>\n          <h5>Work In Tech From Anywhere</h5>\n            </div>\n        {/* Search Input */}\n        <Form>\n            <Col>\n              <Form.Control placeholder={`Search ${this.props.jobs.length} jobs...`}\n              className=\"searchbox\"\n              type=\"text\"\n              name=\"query\"\n              value={this.query}\n              id=\"search-input\"\n              onChange = {_.debounce(this.handleOnInputChange, 150)}/>\n              <i className=\"fas fa-search search-icon\"/>\n              \n              {/* Search Input \n              ReactSearchAutocomplete\n            items={items}\n            onSearch={handleOnSearch}\n            onSelect={handleOnSelect}\n            onFocus={handleOnFocus}\n            autoFocus\n          />\n          */}\n            </Col>\n        </Form>\n        </div>\n      </Row>\n\n      {/* Results Row */}\n        <Row>\n     {this.renderSearchResults()}\n        </Row> \n\n      </div>\n    )\n  }\n}\n\nexport default Search;","/Users/Dylan/Desktop/remoteup2/remoteup1/src/Jobs.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":1,"column":17,"nodeType":"76","messageId":"77","endLine":1,"endColumn":25},{"ruleId":"74","severity":1,"message":"78","line":6,"column":51,"nodeType":"76","messageId":"77","endLine":6,"endColumn":58},{"ruleId":"79","severity":1,"message":"80","line":33,"column":24,"nodeType":"81","messageId":"82","endLine":33,"endColumn"